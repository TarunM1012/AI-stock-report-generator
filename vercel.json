{
  "version": 2,
  "builds": [
    {
      "src": "api/generate-report.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-report",
      "dest": "/api/generate-report.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
} 